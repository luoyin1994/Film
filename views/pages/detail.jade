extends ../layout

block content
  .container
    .row
      .col-md-7(style="overflow:hidden")
        embed(src="#{movie.flash}",allowFullScreen="true",quality="high",width="100%",height="80%",align="middle",type="application/x-shockwave-flash")
      .col-md-5
        dl.dl-horizontal
          each val,key in movie
            - if(key=="flash"||key=="poster") continue
            dt #{key}
            dd #{val}
